{
	"manifest_version": 2,
	"name": "Open BZs",
	"description": "Open BZs in new tabs",
	"version": "0.0.1",
	"permissions": [
		"tabs"
	],
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": false
	},
	"icons": {
		"128": "icons/128x128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://bugzilla.redhat.com/*"
			],
			"js": [
				"open_bz.js"
			]
		}
	]
}
